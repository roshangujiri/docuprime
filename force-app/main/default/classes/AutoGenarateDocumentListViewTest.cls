@isTest
public class AutoGenarateDocumentListViewTest {
     @isTest
    public static void testautoGenerateDocumentFromTemplate() {
        User docupadUser = TestUtils.generateDocuPadUserForTest();
        System.runAs(docupadUser) {
            List<Account> accList = TestDataFactory.createAccounts(1, 'Test Account', true); 
            List<Document_Template__c> tempList = TestDataFactory.createTemplates(1, 'test template', ' ', true);
            List<Document_Content__c> sectionList = TestDataFactory.createSections(2, 'Section', tempList[0].Id, true);
            List<Document_Content__c> fieldList = TestDataFactory.createFields(5,'Field', sectionList, true);
            List<Id> ContIds = new List<Id>();
            ContIds.add(accList[0].Id);
            List<AutoGenerateDocumentWrapperWithIdList> dataList = new List<AutoGenerateDocumentWrapperWithIdList>();
            AutoGenerateDocumentWrapperWithIdList wrapper = new AutoGenerateDocumentWrapperWithIdList();
            wrapper.recordIds = ContIds;
            wrapper.templateName = tempList[0].Name;
            wrapper.attachDocument = true;
            wrapper.sendForEsign = true;
            dataList.add(wrapper);
            AutoGenarateDocumentListViewController.autoGenerateDocumentFromTemplate(dataList);
            Corporate_Document__c cropDoc = [SELECT Id, Name 
                                            FROM Corporate_Document__c 
                                            WHERE Name = 'test template0 - Test Account0' 
                                            LIMIT 1];
            System.assert(cropDoc != Null);
        }
    }
}